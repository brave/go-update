{
  "extends": [
    "local>brave/renovate-config"
  ],
  "packageRules": [
    {
      "description": "AWS SDK v2 updates",
      "matchPackagePatterns": [
        "^github\\.com/aws/aws-sdk-go-v2"
      ],
      "matchUpdateTypes": [
        "patch",
        "minor"
      ],
      "groupName": "AWS SDK v2",
      "prCreation": "not-pending"
    },
    {
      "description": "Sentry SDK updates",
      "matchPackageNames": [
        "github.com/getsentry/sentry-go"
      ],
      "matchUpdateTypes": [
        "patch",
        "minor"
      ],
      "prCreation": "not-pending"
    }
  ]
}
